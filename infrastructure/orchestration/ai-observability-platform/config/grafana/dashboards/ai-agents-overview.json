{
  "dashboard": {
    "id": null,
    "title": "AI Agents Overview",
    "tags": ["ai", "agents", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Total AI Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "count(ai_agent_health_score)",
            "legendFormat": "Total Agents"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "green", "value": 1000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Healthy Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "count(ai_agent_health_score > 0.7)",
            "legendFormat": "Healthy Agents"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Average Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(ai_agent_response_time_seconds)",
            "legendFormat": "Avg Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ai_agent_requests_total{status!=\"200\"}[5m]) / rate(ai_agent_requests_total[5m]) * 100",
            "legendFormat": "Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Agent Health Scores",
        "type": "timeseries",
        "targets": [
          {
            "expr": "ai_agent_health_score",
            "legendFormat": "{{agent_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "pointSize": 3
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(ai_agent_response_time_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ai_agent_response_time_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(ai_agent_response_time_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "ai_agent_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "{{agent_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 17}
      },
      {
        "id": 8,
        "title": "CPU Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "ai_agent_cpu_usage_percent",
            "legendFormat": "{{agent_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 17}
      },
      {
        "id": 9,
        "title": "Intelligence Scores",
        "type": "timeseries",
        "targets": [
          {
            "expr": "ai_agent_intelligence_score",
            "legendFormat": "{{agent_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 26}
      },
      {
        "id": 10,
        "title": "Queue Lengths",
        "type": "timeseries",
        "targets": [
          {
            "expr": "ai_agent_queue_length",
            "legendFormat": "{{agent_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 26}
      }
    ]
  }
}