{
  "dashboard": {
    "id": null,
    "title": "AI Emergence Monitoring",
    "tags": ["ai", "emergence", "collective-intelligence"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "1m",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Emergence Events (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(emergence_events_total[24h])",
            "legendFormat": "Total Events"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Collective Intelligence Score",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(collective_intelligence_score)",
            "legendFormat": "Collective Intelligence"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.8}
              ]
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Swarm Coherence",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(swarm_coherence_score)",
            "legendFormat": "Swarm Coherence"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.85}
              ]
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Phase Transition Indicator",
        "type": "stat",
        "targets": [
          {
            "expr": "max(phase_transition_indicator)",
            "legendFormat": "Phase Transition"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "red", "value": 0.9}
              ]
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Emergence Score Trends",
        "type": "timeseries",
        "targets": [
          {
            "expr": "emergence_score",
            "legendFormat": "{{emergence_type}} - {{cluster_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "pointSize": 5
            },
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Emergence Events by Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(emergence_events_total[5m])",
            "legendFormat": "{{emergence_type}} - {{magnitude}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "bars",
              "barAlignment": 0
            },
            "unit": "ops"
          }
        },
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 18}
      },
      {
        "id": 7,
        "title": "System Complexity Measures",
        "type": "timeseries",
        "targets": [
          {
            "expr": "system_complexity_measure",
            "legendFormat": "{{measurement_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 18}
      },
      {
        "id": 8,
        "title": "Information Flow Rates",
        "type": "timeseries",
        "targets": [
          {
            "expr": "information_flow_rate",
            "legendFormat": "{{source_group}} â†’ {{target_group}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "binBps"
          }
        },
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 9,
        "title": "Network Analysis",
        "type": "table",
        "targets": [
          {
            "expr": "system_complexity_measure{measurement_type=\"network_density\"}",
            "legendFormat": "Network Density",
            "format": "table"
          },
          {
            "expr": "system_complexity_measure{measurement_type=\"clustering_coefficient\"}",
            "legendFormat": "Clustering Coefficient",
            "format": "table"
          },
          {
            "expr": "system_complexity_measure{measurement_type=\"entropy\"}",
            "legendFormat": "Entropy",
            "format": "table"
          }
        ],
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 28}
      }
    ]
  }
}